@model AndroidBinderator.BindingProjectModel
@using System
@using System.Linq

  <ItemGroup>
  @foreach (var art in @Model.MavenArtifacts)
  {
    <AndroidLibrary Include="..\..\externals\@(art.MavenGroupId)\@(art.MavenArtifactId).@(art.MavenArtifactPackaging)" Bind="@(Model.ShouldBindArtifact.ToString())" />
  }
  </ItemGroup>
